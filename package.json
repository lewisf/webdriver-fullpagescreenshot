{
  "name": "webdriver-fullscreenshot",
  "version": "0.0.1",
  "description": "Save a full page screenshot with any driver using Webdriver",
  "main": "built/saveFullPageScreenshot.js",
  "scripts": {
    "test": "node test-compiled.js",
    "watch": "babel test.js --watch --out-file test-compiled.js & babel src --watch --out-dir built",
    "selenium": "selenium-standalone start 1>/dev/null",
    "selenium:install": "selenium-standalone install"
  },
  "dependencies": {
    "gm": "^1.17.0",
    "webdriverio": "3.2.1",
    "selenium-standalone": "^4.4.0",
    "q": "^1.4.1"
  },
  "author": "Lewis Chung",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.0.15",
    "babel-core": "^6.0.15",
    "babel-plugin-transform-object-assign": "^6.0.14",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-stage-2": "^6.0.15"
  }
}
